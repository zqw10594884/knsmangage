<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.zqw.bean">
	<class name="User" table="_user" lazy="true">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="name" type="string" column="_name" />
		<property name="realName" type="string" column="_realName" />
		<property name="password" type="string" column="_password" />
		<property name="authority" type="int" column="_authority" />
	</class>

</hibernate-mapping>


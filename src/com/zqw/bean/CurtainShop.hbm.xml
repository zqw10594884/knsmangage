<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.zqw.bean">
	<class name="CurtainShop" table="_curtainshop" lazy="true">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="name" type="string" column="_name" />
		<property name="telephone" type="string" column="_telephone" />
		<property name="address" type="string" column="_address" />
		<property name="owner" type="int" column="_owner" />
	</class>
</hibernate-mapping>


<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.zqw.bean">
	<class name="VIPUser" table="_VIPUser" lazy="true">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="name" type="string" column="_name" />
		<property name="tel" type="string" column="_tel" />
		<property name="integral" type="int" column="_integral" />
	</class>

</hibernate-mapping>

